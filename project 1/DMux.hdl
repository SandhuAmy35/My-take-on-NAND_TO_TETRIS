/**
 * Demultiplexor:
 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */
CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Nand(a=sel,b=sel,out=s);
    Nand(a=in,b=s,out=a1);
    Nand(a=a1,b=a1,out=a);
    Nand(a=in,b=sel,out=b1);
    Nand(a=b1,b=b1,out=b);  
}
