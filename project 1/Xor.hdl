/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a,b=n2,out=a1);
    Nand(a=a,b=a,out=n1);
    Nand(a=n1,b=b,out=b1);
    Nand(a=b,b=b,out=n2);
    Nand(a=a1,b=b1,out=out);
}